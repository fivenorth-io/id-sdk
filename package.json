{
	"name": "@fivenorth/id-sdk",
	"version": "0.1.3",
	"description": "TypeScript/JavaScript SDK for programmatically interacting with the 5N ID service",
	"keywords": [
		"5n",
		"id",
		"sdk",
		"credentials",
		"verification",
		"identity",
		"canton",
		"fivenorth"
	],
	"author": "support@fivenorth.io",
	"license": "UNLICENSED",
	"homepage": "https://github.com/fivenorth-io/id-sdk#readme",
	"bugs": {
		"url": "https://github.com/fivenorth-io/id-sdk/issues"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"devDependencies": {
		"@types/bun": "latest",
		"@types/node": "^20.0.0",
		"tsx": "^4.7.0",
		"typescript": "^5"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fivenorth-io/id-sdk.git"
	},
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist && tsc --emitDeclarationOnly",
		"prepublishOnly": "bun run build",
		"generate-types": "openapi-typescript ../kyc-backend/api-openapi.json -o src/generated-types.ts",
		"test": "tsx test.ts",
		"test:bun": "bun run test.ts"
	},
	"type": "module",
	"types": "dist/index.d.ts",
	"dependencies": {
		"openapi-typescript": "^7.0.0"
	}
}